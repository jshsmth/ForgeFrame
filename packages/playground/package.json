{
  "name": "@forgeframe/playground",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "concurrently \"npm run dev:parent\" \"npm run dev:child\"",
    "dev:parent": "vite --config vite.config.parent.ts",
    "dev:child": "vite --config vite.config.child.ts",
    "build": "npm run build:parent && npm run build:child",
    "build:parent": "vite build --config vite.config.parent.ts",
    "build:child": "vite build --config vite.config.child.ts"
  },
  "dependencies": {
    "forgeframe": "*"
  }
}
